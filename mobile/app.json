{
  "expo": {
    "name": "WellBot",
    "slug": "wellbot-mental-health",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "wellbot",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wellbot.mentalhealth"
    },
    "android": {
      "package": "com.wellbot.mentalhealth",
      "adaptiveIcon": {
        "backgroundColor": "#667eea",
        "foregroundImage": "./assets/images/android-icon-foreground.png"
      },
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "add1fffa-0cc7-48c8-9521-3294c2581d32"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/add1fffa-0cc7-48c8-9521-3294c2581d32"
    }
  }
}
